<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - JensensWebApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/StylingBody.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/JensensWebApp.styles.css" asp-append-version="true" />   
    <link rel="stylesheet" href="~/css/footerstyle.css" asp-append-version="true" />

    
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-white border-bottom box-shadow fixed-top">

            <div class="container">
                
                <a class="navbar-brand" href="#">

                    <img src="~/Images/Img1.png" alt="Atlas Portal" style="width: 50%">

                </a>

               
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link" href="#">Hem</a>
                    </li>
                    <li class="nav-item dropdown">
                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown"
                           aria-haspopup="true" aria-expanded="false">
                            Mer

                        </a>
                        <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                            <a class="dropdown-item" href="#">Världen</a>
                            <a class="dropdown-item" href="#">Fotboll</a>
                            <a class="dropdown-item" href="#">Mat</a>
                            <a class="dropdown-item" href="#">Mode</a>
                            <a class="dropdown-item" href="#">Rymdskepp</a>
                        </div>
                    </li>
                </ul>

                    <ul class="navbar-nav mr-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="#">Nyheter</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Sport</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Kultur</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Livsstil</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Teknologi</a>
                        </li>
                    </ul>
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item">
                            <a class="nav-link" href="#">Logga in</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">Prenumenera</a>
                        </li>
                    </ul>
                </div>
        </nav>
    </header>
   
    <div class="mt-lg-3">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>

    <footer class="footer text-white">

        <!--&copy; 2023 - JensensWebApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>-->

        <div class="align-top text-center">

            <dl class="row m-0">
                <dt class="col"><a class="footer-nav-headline" href="#">Hälsa</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Samhälle & Konflikter</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Miljö</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Vetenskap & Teknik</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Livsstil & Fritid</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Ekonomi</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Religion</a></dt>
                <dt class="col"><a class="footer-nav-headline" href="#">Idrott</a></dt>

            </dl>
        </div>

        <div class="elemento1"></div>
        
        <div class="row secondary-footer">

            <div class="signup col">
                <p class="signup-text">Come on! Sign up for some more mjau-content</p> <button class="btn-footer bf-1">Sign up for our email</button>
            </div>
    
            <div class="gridfooter col">

                <div class="row">
                    <div class="footer-box col">
                        <dl class="dl">
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Hjälp</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Klagomål</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Rättelse</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Kontakta oss</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Kakor</a></dt>
                            

                        </dl>
                    </div>

                    <div class="footer-box col">
                        <dl class="dl">
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Alla teman</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Jobba hos oss</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Journalister</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Arkiv</a></dt>
                            

                        </dl>
                    </div>

                    <div class="footer-box col">
                        <dt class="dt-el"><a href="#" class="footer-box-el smaller">Facebook</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">YouTube</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Instagram</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">LinkedIn</a></dt>
                            <dt class="dt-el"><a href="#" class="footer-box-el smaller">Twitter</a></dt>
                    </div>

                    <div class="col">
                        <h2>Support us!</h2>
                        <button class="btn-footer bf-2">Support us</button>
                    </div>
                </div>
                
            </div>
        </div>

        <p>&#169; 2023 Grupp3 WebbNews Mjaumjau. All rights reserved.</p>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>